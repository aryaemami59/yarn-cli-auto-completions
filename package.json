  "devDependencies": {
    "@aryaemami59/eslint-config": "^0.0.7",
    "@aryaemami59/prettier-config": "^0.0.7",
    "@aryaemami59/tsconfig": "^0.0.7",
    "@aryaemami59/vitest-config": "^0.0.7",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.3",
    "@yarnpkg/cli": "^4.12.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/aryaemami59"
    }
  ],
  "homepage": "https://github.com/aryaemami59/yarn-cli-auto-completions#readme",
  "keywords": [
    "yarn",
